{
  "name": "AzdoExtension",
  "version": "1.0.0",
  "description": "Azure DevOps extensions that provides some ease to developers",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist && npm run delete-pacakges",
    "delete-pacakges": "rimraf *.vsix",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "XX_postbuild": "npm run package-extension",
    "package": "npm run build && npm run postbuild",
    "package-extension": "tfx extension create --manifest-globs vss-extension.json --rev-version",
    "publish-extension": "tfx extension publish --manifest-globs vss-extension.json extensions/all/**/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoimHossain/azure-devops-governance-extension.git"
  },
  "keywords": [
    "azure-devops",
    "extensions",
    "governance"
  ],
  "author": "Moim Hossain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MoimHossain/azure-devops-governance-extension/issues"
  },
  "homepage": "https://github.com/MoimHossain/azure-devops-governance-extension#readme",
  "dependencies": {
    "azure-devops-extension-api": "~1.157.0",
    "azure-devops-extension-sdk": "~2.0.11",
    "azure-devops-ui": "~2.164.0",
    "react": "~16.8.1",
    "react-dom": "~16.8.1"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "node-sass": "npm:sass@^1.49.7",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  }
}
